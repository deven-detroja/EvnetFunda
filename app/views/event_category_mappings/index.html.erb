<h1>Listing event_category_mappings</h1>

<table>
  <thead>
    <tr>
      <th>Categories</th>
      <th>Sub categories</th>
      <th>Event master</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_category_mappings.each do |event_category_mapping| %>
      <tr>
        <td><%= event_category_mapping.categories %></td>
        <td><%= event_category_mapping.sub_categories %></td>
        <td><%= event_category_mapping.event_master %></td>
        <td><%= link_to 'Show', event_category_mapping %></td>
        <td><%= link_to 'Edit', edit_event_category_mapping_path(event_category_mapping) %></td>
        <td><%= link_to 'Destroy', event_category_mapping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event category mapping', new_event_category_mapping_path %>
